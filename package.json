{
  "name": "react-notion-table",
  "version": "0.1.7",
  "description": "Notion-like table component implementation for React",
  "main": "dist/index.js",
  "repository": "https://github.com/opa-oz/react-notion-table.git",
  "author": "Vladimir <opaozhub@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "parcel example/index.html --open",
    "build": "parcel build src/index.js",
    "build:example": "cp -R dist/ example/",
    "clean": "rm -rf dist .cache coverage",
    "lint": "eslint src/**/*.js",
    "test": "jest",
    "coverage": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:updateSnapshots": "jest --updateSnapshot",
    "release": "standard-version"
  },
  "contributors": [
    "Contributors (https://github.com/opa-oz/react-notion-table/graphs/contributors)"
  ],
  "files": [
    "package.json",
    "dist/*"
  ],
  "bugs": {
    "url": "https://github.com/opa-oz/react-notion-table/issues"
  },
  "keywords": [
    "react",
    "table",
    "component",
    "editable",
    "notion"
  ],
  "lint-staged": {
    "*.{js,jsx}": [
      "pretty-quick --staged",
      "eslint src/ --fix",
      "git add"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "immutable": "^4.0.0-rc.12",
    "jest-css-modules": "^2.1.0",
    "jest-transform-stub": "^2.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "shortid": "^2.2.14"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.9.0",
    "coveralls": "^3.0.6",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^3.0.4",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.3",
    "parcel-bundler": "^1.12.3",
    "postcss-modules": "^1.4.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "react-test-renderer": "^16.9.0",
    "sass": "^1.22.10",
    "standard-version": "^7.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
